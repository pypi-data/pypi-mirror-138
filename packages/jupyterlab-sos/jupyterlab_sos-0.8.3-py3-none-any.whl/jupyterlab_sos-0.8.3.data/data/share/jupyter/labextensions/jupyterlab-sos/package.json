{
  "name": "jupyterlab-sos",
  "version": "0.8.2",
  "description": "JupyterLab extension for SoS workflow engine and polyglot notebook",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/vatlab/jupyterlab-sos",
  "bugs": {
    "url": "https://github.com/vatlab/jupyterlab-sos/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Bo Peng",
    "email": "ben.bob@gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/vatlab/jupyterlab-sos.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_sos/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.12",
    "@jupyterlab/apputils": "^3.1.12",
    "@jupyterlab/codemirror": "^3.1.12",
    "@jupyterlab/console": "^3.1.12",
    "@jupyterlab/coreutils": "^5.1.0",
    "@jupyterlab/docregistry": "^3.1.12",
    "@jupyterlab/notebook": "^3.1.12",
    "@jupyterlab/services": "^6.1.12",
    "@jupyterlab/settingregistry": "^3.1.12",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/commands": "^1.12.0",
    "@lumino/disposable": "^1.4.3",
    "transient-display-data": "^0.4.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.0",
    "@types/codemirror": "0.0.97",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "codemirror": "^5.53.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.15.4",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "outputDir": "jupyterlab_sos/labextension",
    "_build": {
      "load": "static/remoteEntry.959a7b9a218d10ed3796.js",
      "extension": "./extension",
      "style": "./style"
    }
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.1",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
